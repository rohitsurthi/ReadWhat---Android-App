        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/theme_color_black</item>
        <item name="colorPrimaryVariant">@color/theme_color_grey</item>
        <item name="colorOnPrimary">@color/theme_color_white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/theme_color_black</item>
        <item name="colorSecondaryVariant">@color/theme_color_grey</item>
        <item name="colorOnSecondary">@color/theme_color_white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>